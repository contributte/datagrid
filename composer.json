{
	"name": "ublaboo/datagrid",
	"type": "library",
	"description": "DataGrid for Nette Framework: filtering, sorting, pagination, tree view, table view, translator, etc",
	"keywords": ["datagrid", "grid", "nette"],
	"homepage": "https://ublaboo.org/datagrid",
	"license": ["MIT"],
	"support": {
		"issues": "https://github.com/ublaboo/datagrid/issues"
	},
	"authors": [
		{
			"name": "Pavel Janda",
			"homepage": "http://paveljanda.com"
		}
	],
	"autoload": {
		"psr-4": {
			"Ublaboo\\DataGrid\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Ublaboo\\DataGrid\\Tests\\": "tests"
		}
	},
	"require": {
		"php": "^5.6|^7.0",
		"nette/application": "^2.3",
		"nette/utils": ">=2.4.0",
		"nette/forms": "^2.3",
		"ublaboo/responses": "~1.0.0|~2.0.0",
		"symfony/property-access": "~3.0"
	},
	"require-dev": {
		"nette/tester": "^1.6.1",
		"tracy/tracy": "^2.3.0",
		"mockery/mockery": "~0.9",
		"tharos/leanmapper": "~2.2.0",
		"nextras/orm": "~2.0",
		"doctrine/orm": "~2.5",
		"nette/database": "^2.3",
		"dibi/dibi": "^3.0"
	}
}
